{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "User Data Format",
  "definitions" : {
    "UserDataFormat" : {
      "type": "object",
      "required": ["id", "first_name", "last_name", "avatar"],
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "first_name": {
          "type": "string"
        },
        "last_name": {
          "type": "string"
        },
        "avatar": {
          "type": "string"
        }
      }
    }
  }
}
