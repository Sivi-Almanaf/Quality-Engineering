{
  "$schema": "http://json-schema.org/draft-06/schema#",
  "title": "User Data Format",
  "definitions" : {
    "UserDataFormat" : {
      "type": "object",
      "required": ["id", "firstName", "lastName", "email", "password", "phone", "userStatus"],
      "additionalProperties": false,
      "properties": {
        "id": {
          "type": "integer"
        },
        "firstName": {
          "type": "string"
        },
        "lastName": {
          "type": "string"
        },
        "email": {
          "type": "string"
        },
        "password": {
          "type": "string"
        },
        "phone": {
          "type": "string"
        },
        "userStatus": {
          "type": "integer"
        }
      }
    }
  }
}